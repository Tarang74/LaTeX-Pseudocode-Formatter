ALGORITHM SelectSort(A[0..n-1])
// Selection sort algorithm
for i <- 0 to n - 1 do
    SmallSub = i
    for j <- i + 1 to n - 1 do
        if A[j] < A[SmallSub]
            SmallSub = j

    temp = A[i]
    A[SmallSub] = temp

return A
:{6:7}O\left( 1 \right)
:{5:7}O\left( n \right)
:{3:10}O\left( n^2 \right)
